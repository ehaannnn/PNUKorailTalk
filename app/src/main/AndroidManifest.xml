<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.korailtalk" >

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CheckSessionActivity"
            android:label="@string/title_activity_check_session" />
        <activity
            android:name=".PaidTicketSearch"
            android:label="@string/title_activity_paied_ticket_search" />
        <activity android:name=".MyPageActivity" />
        <activity
            android:name=".DatePickerActivity"
            android:label="@string/title_activity_date_picker" />
        <activity android:name=".TicketCancelActivity" />
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".TrainSearch"
            android:label="TrainSearch" />
        <activity
            android:name=".SeatSearch"
            android:label="SeatSearch" />
        <activity

            android:name=".AvailableTrainLists"
            android:label="AvailableTrainLists" />

        <activity

            android:name=".UnPaidTicketSearch"
            android:label="UnPaidTicketSearch" />


        <activity
            android:name=".TicketHistoryActivity"
            android:label="TicketHistoryActivity"
            android:theme="@style/Theme.AppCompat"/>


    </application>

</manifest>
